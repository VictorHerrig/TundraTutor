<control:CustomWindow x:Class="TutorWindows.Credits"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:control="clr-namespace:TundraControls;assembly=TundraControls"
        Title="Credits" Width="292.131" Height="396.989" Icon="Icons/Icon.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="47*"/>
            <RowDefinition Height="214*"/>
            <RowDefinition Height="43*"/>
            <RowDefinition Height="190*"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Text="Brought to you by " HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="26"/>
        <Image Source="Icons\TundraLogo.png" VerticalAlignment="Bottom" Grid.Row="1"/>
        <TextBlock Grid.Row="2" Text="Developers: " VerticalAlignment="Top" FontSize="16"/>
        <StackPanel Grid.Row="3">
            <!--Sign your name here!!-->

            <TextBlock x:Name="Chris" Text="Christopher Ertl" FontSize="22" FontFamily="Freestyle Script"/>
            <TextBlock x:Name="Makena" Text="Makena Dettmann" FontFamily="Curlz MT" FontSize="22" Foreground="#FFFF00F3"/>
            <TextBlock x:Name="Victor" Text="Victor Herrig" FontFamily="Vivaldi" FontSize="24" Foreground="#FF4B2E2B"/>
        </StackPanel>

    </Grid>
</control:CustomWindow>